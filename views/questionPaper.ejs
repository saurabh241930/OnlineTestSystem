<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>test</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css" />
<link href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet">
</head>
<body data-spy="scroll" data-target=".new-question-button">
  <h1 style="text-align: center;background-color: yellow;">timing</h1>
  <div class="row" id="main-class">
    <div class="col-lg-9  col-md-9 col-sm-9"  style="background-color: red; color:white">
      <form action="/submitPaper/<%=testId%>"  method="post" id="form1">
        <div class="main-form" id="test-form"> 
            <%questions.forEach(function(question){%>
              <div style="border:solid 1px black" class="questionClass">
                <h1  style=""><%=question.QuestionLink%> </h1><br><br>
                  <form action="" id="formId1">
                     <label><input type="radio" name="gender" value="show" onchange="setColor('<%=question._id%>')"> Male</label><br>
                     <label><input type="radio" name="gender" value="show" onchange="setColor('<%=question._id%>')"> female</label><br> 
                     <label><input type="radio" name="gender" value="show" onchange="setColor('<%=question._id%>')"> others</label><br>
                  </form>
              </div>
            <%})%>
        </div>
        <button type="submit">submit</button>
      </form>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-3" style="background-color: blue; color:white">
      <%questions.forEach(function(question){%>
       <a type="button" href="/test/question/<%=question._id%>" class="new-question-button" id="<%=question._id%>">button</a>
      <%})%>
    </div>
  </div>
</body>
</html>


